{
  "expo": {
    "name": "Anagkazo Pastoral Point",
    "slug": "anagkazo-pastoral-app",
    "version": "1.5",
    "orientation": "portrait",
    "icon": "./assets/imgs/rsz_logo_anagkazo_icon.png",
    "splash": {
      "image": "./assets/imgs/logo_anagkazo_copy.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "package": "com.sabetech.anagkazo_pastoral_point",
      "versionCode": 5,
      "permissions": [
        "CAMERA", "VIBRATE", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
